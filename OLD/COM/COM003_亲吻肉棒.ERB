@COM_ABLE3
    IF FLAG:自机部位 == 1 && FLAG:目标 != MASTER && FLAG:目标部位 == 5 && BASE:MASTER:体力 > 0
        IF MASTER != FLAG:本机
            IF COMABLE(MASTER,4) == 1
                FLAG:指令可行 = 1
                SIF CFLAG:(FLAG:目标):Ｐ插入中 == 1
                    FLAG:指令可行 = 2
                SIF CFLAG:MASTER:Ｍ插入中 == 1
                    FLAG:指令可行 = 2
                SIF CFLAG:MASTER:Ｍ状态 > 0
                    FLAG:指令可行 = 3
            ELSE
                FLAG:指令可行 = 0
            ENDIF
        ELSE
            FLAG:指令可行 = 1
            SIF CFLAG:(FLAG:目标):Ｐ插入中 == 1
                FLAG:指令可行 = 2
            SIF CFLAG:MASTER:Ｍ插入中 == 1
                FLAG:指令可行 = 2
            SIF CFLAG:MASTER:Ｍ状态 > 0
                FLAG:指令可行 = 3
        ENDIF
    ELSE
        FLAG:指令可行 = 0
    ENDIF
    
@COM_KEEP_ABLE3
    IF BASE:MASTER:体力 > 0
        FLAG:指令继续可行 = 0
    ENDIF


@COM_PRE_3
    #DIM LCOUNT 
    #DIM LCOUNT_2
    #DIM MEMO_TARGET
    MEMO_TARGET = CFLAG:MASTER:Ｍ对象

    CALL COM_PRE_COMMON(1, 1, 5, "Ｍ插入中", "Ｐ插入中")
    
    CFLAG:MEMO_TARGET:Ｐ插入中 = 0
    CFLAG:MASTER:Ｍ插入中 == 1
    CFLAG:MASTER:Ｍ状态 = 1
    CFLAG:MASTER:Ｍ对象 = FLAG:目标
    CFLAG:MASTER:Ｍ指令 = 3
    CFLAG:MASTER:Ｍ接触 = 5

@COM_3
