@SOURCE_CHECK_EX_CHECK_MASTER; 高潮运算
    #DIM LCOUNT
    #DIMS STOP_EX
    #DIMS ENDURE_EX
    #DIM EXLEVEL = 0
    #DIM OTHER_JUEL
    #DIM EX_LV
    #DIM EX_LV_TOTAL

    FOR LCOUNT, 101, 111
        SELECTCASE PALAM:MASTER:LCOUNT
            CASE IS >= 500000
                EX_LV = 10
            CASE IS >= 400000
                EX_LV = 9
            CASE IS >= 300000
                EX_LV = 8
            CASE IS >= 250000
                EX_LV = 7
            CASE IS >= 200000
                EX_LV = 6
            CASE IS >= 150000
                EX_LV = 5
            CASE IS >= 100000
                EX_LV = 4
            CASE IS >= 50000
                EX_LV = 3
            CASE IS >= 20000
                EX_LV = 2
            CASE IS >= 10000
                EX_LV = 1
            CASE IS < 10000
                EX_LV = 0
        ENDSELECT
        CFLAG:MASTER:(LCOUNT + 230) = EX_LV
        EX_LV = 0
    NEXT