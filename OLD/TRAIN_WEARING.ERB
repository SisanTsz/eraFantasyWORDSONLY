@TRAIN_WEARING(ARG,ARG:1)
    #DIM LCOUNT 
    #DIM WTARGET
    WTARGET = ARG
    FOR LCOUNT, 1, 11
        TRYCALLFORM TRAIN_WEARING_{LCOUNT}(WTARGET)
    NEXT

@TRAIN_WEARING_7(ARG,ARG:1)
    #DIM WTARGET
    #DIM LCOUNT
    WTARGET = ARG
    SELECTCASE CFLAG:WTARGET:Ｖ调教装备
        CASE 1
            CALL V_SEX_TOY_STICK(WTARGET, 5)
    ENDSELECT

@V_SEX_TOY_STICK(ARG,ARG:1);目标,时间
    #DIM WTARGET
    #DIM SLV 
    #DIM SIZE
    #DIM JLV
    WTARGET = ARG
    SLV = CFLAG:WTARGET:Ｖ调教装备强度
    SZIE = CFLAG:WTARGET:Ｖ调教装备大小
    SLV *= ARG:1
    SLV *= SZIE - ABL:WTARGET:Ｖ日常尺寸 + 1
    SIF SLV < 0
        SLV = 0