@USERSHOP
    IF RESULT == 900
        ALIGNMENT LEFT
        SAVEGAME
    ELSEIF RESULT == 901
        ALIGNMENT LEFT
        LOADGAME
    ELSEIF RESULT == 902
        CLEARLINE LINECOUNT
        ;CALL SETTING
    ELSEIF RESULT == 200
        IF TARGET >= 1
            CLEARLINE LINECOUNT
            CALL TRAIN_MAIN
        ELSE    
            CUSTOMDRAWLINE -
            PRINTL 选择调教对象
            CALL OTHER_CHARACTER_LIST, 1
        ENDIF
    ELSEIF RESULT == 201
        CALL CHARACTER_LIST, 1, 1
    ELSEIF RESULT == 202
        CALL TAKE_A_BREAK
    ELSEIF RESULT == 203
        CUSTOMDRAWLINE -
        PRINTL 选择修改放置调教的角色
        CALL OTHER_CHARACTER_LIST, 3
    ELSEIF RESULT == 204
        CUSTOMDRAWLINE -
        PRINTFORML 现在的调教对象是%NAME:TARGET%
        CALL OTHER_CHARACTER_LIST, 1
    ELSEIF RESULT == 205
        CUSTOMDRAWLINE -
        IF ASSI < 0
            PRINTL 现在没有助手
            CALL OTHER_CHARACTER_LIST, 2
        ELSEIF ASSI >= 0
            PRINTFORML 现在的助手是%NAME:ASSI%
            CALL OTHER_CHARACTER_LIST, 2
        ENDIF
    ELSEIF RESULT == 301
        CALL CHARACTER_LIST, 2, 1
    ELSEIF RESULT == 3441
        IF FLAG:5 == 1
            FLAG:5 = 0
        ELSE
            FLAG:5 = 1
        ENDIF
    ENDIF