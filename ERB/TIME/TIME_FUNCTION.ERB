@HOUR
    #FUNCTIONS
    SELECTCASE FLAG:时
        CASE 1
            RETURNF "01"
        CASE 2
            RETURNF "02"
        CASE 3
            RETURNF "03"
        CASE 4
            RETURNF "04"
        CASE 5
            RETURNF "05"
        CASE 6
            RETURNF "06"
        CASE 7
            RETURNF "07"
        CASE 8
            RETURNF "08"
        CASE 9
            RETURNF "09"
        CASE 10 TO 24
            RETURNF TOSTR(FLAG:时)
    ENDSELECT

@MINUTE
    #FUNCTIONS
    SELECTCASE FLAG:分
        CASE 1
            RETURNF "01"
        CASE 2
            RETURNF "02"
        CASE 3
            RETURNF "03"
        CASE 4
            RETURNF "04"
        CASE 5
            RETURNF "05"
        CASE 6
            RETURNF "06"
        CASE 7
            RETURNF "07"
        CASE 8
            RETURNF "08"
        CASE 9
            RETURNF "09"
        CASE 10 TO 60
            RETURNF TOSTR(FLAG:分)
    ENDSELECT