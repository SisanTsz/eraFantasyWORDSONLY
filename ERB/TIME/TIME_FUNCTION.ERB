@MOUNTH
    #FUNCTIONS
    SELECTCASE FLAG:月
        CASE 1
            RETURNF "一"
        CASE 2
            RETURNF "二"
        CASE 3
            RETURNF "三"
        CASE 4
            RETURNF "四"
        CASE 5
            RETURNF "五"
        CASE 6
            RETURNF "六"
        CASE 7
            RETURNF "七"
        CASE 8
            RETURNF "八"
        CASE 9
            RETURNF "九"
        CASE 10
            RETURNF "十"
        CASE 11
            RETURNF "十一"
        CASE 12
            RETURNF "十二"
    ENDSELECT

