@GAL_SHOW
    #DIM LCOUNT
    #DIMS PRE
    #DIM LNUM 
    LNUM = FLAG:MAINUI行数
    FOR LCOUNT, 0, LNUM
        PRE = MAINUI%TOSTR(LCOUNT+1)%
        PRINTFORML %STR:PRE%
    NEXT

@GAL_BUTTON
    #DIM LCOUNT
    #DIMS PRE
    #DIM BNUM
    BNUM = 1
    STR:GAMEBUTTON1 = 互动
    STR:GAMEBUTTON2 = 交互
    STR:GAMEBUTTON3 = 查看
    STR:GAMEBUTTON4 = 能力
    STR:GAMEBUTTON5 = 移动
    STR:GAMEBUTTON6 = 系统
    FOR LCOUNT, 7, 21
        PRE = GAMEBUTTON%TOSTR(LCOUNT)%
        STR:PRE = 
    NEXT
    FOR LCOUNT, 1, 21
        PRE = GAMEBUTTON%TOSTR(LCOUNT)%
        IF STR:PRE != ""
            PRINTBUTTON @"%STR:PRE%", 1200 + LCOUNT
            PRINTFORM %SPACE(9)%
        ENDIF
    NEXT

@GAL_CHOICE
    #DIM LCOUNT
    #DIMS PRE
    #DIM BNUM
    BNUM = 1
    SELECTCASE FLAG:CHOICE界面
        CASE 0
            CALL PRINT_LINE(5)
        CASE 1201
            STR:CHOICE1 = 对话
            STR:CHOICE2 = 抱抱
            FOR LCOUNT, 3, 21
                PRE = CHOICE%TOSTR(LCOUNT)%
                STR:PRE = 
            NEXT
            CALL PRINT_LINE(4)
            FOR LCOUNT, 1, 21
                PRE = CHOICE%TOSTR(LCOUNT)%
                IF STR:PRE != ""
                    PRINTBUTTON @"%STR:PRE%", 1100 + LCOUNT
                    PRINTFORM %SPACE(3)%
                ENDIF
            NEXT
        CASEELSE
            CALL PRINT_LINE(4)
            FOR LCOUNT, 1, 21
                PRE = CHOICE%TOSTR(LCOUNT)%
                IF STR:PRE != ""
                    PRINTBUTTON @"%STR:PRE%", 1100 + LCOUNT
                    PRINTFORML {1100 + LCOUNT}
                    PRINTFORM %SPACE(3)%
                ENDIF
            NEXT
    ENDSELECT