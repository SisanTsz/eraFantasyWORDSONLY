@SETTING, ARGS ;总设置
    #DIM LCOUNT
    REDRAW 0
    PRINTFORML [1] XP系统设置
    PRINTFORML [2] 游戏系统设置
    PRINTL
    PRINTFORML [0] 返回
    INPUT
    IF RESULT == 1
        CALL XPSETTING
    ELSEIF RESULT == 2
        CALL GAMESETTING
    ELSEIF RESULT == 0
    ENDIF

@XPSETTING ;xp屏蔽
    DO
    PRINTFORML 按照标签屏蔽事件以及数据显示
    PRINTFORML 未屏蔽：O 已屏蔽：X
    PRINTL
;IF GLOBAL:102 == 1
;    GLOBAL:101 = 1
;ENDIF
    IF GLOBAL:104 == 1
        GLOBAL:103 = 1
    ENDIF
    IF GLOBAL:101 == 1
        PRINTFORM [1] X耻垢
    ELSE
        PRINTFORM [1] O耻垢
    ENDIF
    IF GLOBAL:102 == 1
        PRINTFORM [2] X包茎
    ELSE
        PRINTFORM [2] O包茎
    ENDIF
    IF GLOBAL:103 == 1
        PRINTFORM [3] X体毛
    ELSE
        PRINTFORM [3] O体毛
    ENDIF
    IF GLOBAL:104 == 1
        PRINTFORM [4] X阴毛
    ELSE
        PRINTFORM [4] O阴毛
    ENDIF
    PRINTL
    PRINTFORML [0] 返回
    INPUT
    IF RESULT == 1
        IF GLOBAL:101 == 0
            GLOBAL:101 = 1
        ELSE
            GLOBAL:101 = 0
        ENDIF
    ELSEIF RESULT == 2
        IF GLOBAL:102 == 0
            GLOBAL:102 = 1
        ELSE
            GLOBAL:102 = 0
        ENDIF
    ELSEIF RESULT == 3
        IF GLOBAL:103 == 0
            GLOBAL:103 = 1
        ELSE
            GLOBAL:103 = 0
        ENDIF
    ELSEIF RESULT == 4
        IF GLOBAL:104 == 0
            GLOBAL:104 = 1
        ELSE
            GLAOBAL:104 = 0
        ENDIF
    ELSEIF RESULT == 0
        BEGIN FIRST
    ENDIF
    SAVEGLOBAL
    LOOP 1