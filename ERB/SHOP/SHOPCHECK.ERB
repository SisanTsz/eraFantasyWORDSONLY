@SHOPCHECK
IF RESULT == 900
    ALIGNMENT LEFT
    SAVEGAME
ELSEIF RESULT == 901
    ALIGNMENT LEFT
    LOADGAME
ELSEIF RESULT == 902
    CLEARLINE LINECOUNT
    CALL SETTING
ELSEIF RESULT == 200
    IF TARGET >= 1
        CLEARLINE LINECOUNT
        CALL TRAIN
    ELSE
        CUSTOMDRAWLINE -
        PRINTL 选择调教对象
        CALL OTHERCHARACTERLIST, 1
    ENDIF
ELSEIF RESULT == 201
    CALL CHARACTERLIST, 1
ELSEIF RESULT == 202
    CALL TAKEABREAK
ELSEIF RESULT == 203
    CUSTOMDRAWLINE -
    PRINTL 选择修改放置调教的角色
    CALL OTHERCHARACTERLIST, 3
ELSEIF RESULT == 204
    CUSTOMDRAWLINE -
    PRINTFORML 现在的调教对象是%NAME:TARGET%
    CALL OTHERCHARACTERLIST, 1
ELSEIF RESULT == 205
    CUSTOMDRAWLINE -
    IF ASSI < 0
        PRINTL 现在没有助手
        CALL OTHERCHARACTERLIST, 2
    ELSEIF ASSI >= 0
        PRINTFORML 现在的助手是%NAME:ASSI%
        CALL OTHERCHARACTERLIST, 2
    ENDIF
ELSEIF RESULT == 300
    CALL OTHERCHARACTERLIST, 4
ELSEIF RESULT == 301
    CALL ITEMLIST
ELSEIF RESULT == 302
    CALL BUILDINGLIST
ENDIF