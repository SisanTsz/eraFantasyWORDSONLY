@CONFIRM_LOCATION(ARG, ARG:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6, ARG:7, ARG:8, ARG:9, ARG:10)
    #FUNCTION
    #DIM CNUM
    #DIM LCOUNT
    #DIM OUT = 0
    #DIMS PRE 
    CNUM = ARG
    PRE =
    FOR LCOUNT, 1, 11
        IF TOSTR(ARG:LCOUNT) != ""
            PRE = 位置%TOSTR(LCOUNT)%
            SIF CFLAG:CNUM:PRE != ARG:LCOUNT
                OUT = 1
        ENDIF
    NEXT
    IF OUT == 1
        RETURNF 0
    ELSE
        RETURNF 1
    ENDIF


@LOCATION_LIST(ARG,ARGS)
    #DIM CNUM
    #DIM LNUM
    #DIM LCOUNT
    #DIMS CODE
    #DIMS CODE_1
    #DIMS PRE
    #DIMS LIST
    CNUM = ARG
    FOR LCOUNT, 10, 0
        PRE = 位置%TOSTR(LCOUNT)%
        IF CFLAG:CNUM:PRE != 0
            LNUM = LCOUNT
            BREAK
        ENDIF
    NEXT
    PRINTFORML {LNUM}
    CODE = 
    LNUM = 7
    FOR LCOUNT, 1, LNUM
        PRE = 位置%TOSTR(LCOUNT)%
        IF LCOUNT != 10
            CODE = %CODE%%TOSTR(CFLAG:CNUM:PRE)%.
        ELSE
            CODE = %CODE%%TOSTR(CFLAG:CNUM:PRE)%
        ENDIF
    NEXT
    
    PRINTFORML %CODE%
    PRINTFORML %CODE_1%
    FOR a, 1, 100
        CODE_1 = %CODE%%TOSTR(a)%
        FOR b, 0, (10-LNUM)
            CODE_1 = %CODE_1%.0
        NEXT
        IF LOCATION_NAME(CODE_1) != "错误"
            PRINTFORM %LOCATION_NAME(CODE_1)%
            PRINTL 1
        ELSE
            ;PRINTFORML %CODE_1%
        ENDIF
    NEXT
    PRINTFORML {LNUM}
    PRINTFORML %CODE%
    PRINTFORML %CODE_1%