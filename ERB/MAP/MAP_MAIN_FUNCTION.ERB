@LOCATION(ARG) ;角色编号
    #FUNCTIONS
    #DIMS LNAME
    LNAME = %LOCATION_NAME(LOCATION_CODE(ARG))%
    RETURNF LNAME
    
@LOCATION_CODE(ARG)
    #FUNCTIONS
    #DIM LCOUNT
    #DIM CNUM
    #DIMS CODE
    #DIMS PRE
    CNUM = ARG
    CODE =
    FOR LCOUNT, 1, 11
        PRE =  位置%TOSTR(LCOUNT)%
        IF LCOUNT != 10
            CODE = %CODE%%TOSTR(CFLAG:CNUM:PRE)%.
        ELSE
            CODE = %CODE%%TOSTR(CFLAG:CNUM:PRE)%
        ENDIF
    NEXT
    RETURNF CODE


@TP(ARG, ARG:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6, ARG:7, ARG:8, ARG:9, ARG:10)
    #DIM CNUM
    #DIM LCOUNT
    #DIMS PRE 
    CNUM = ARG
    PRE =
    FOR LCOUNT, 1, 11
        PRE = 位置%TOSTR(LCOUNT)%
        IF TOSTR(ARG:LCOUNT) != ""
            CFLAG:CNUM:PRE = ARG:LCOUNT
        ELSE
            CFLAG:CNUM:PRE = 0
        ENDIF
    NEXT

