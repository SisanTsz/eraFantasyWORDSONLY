@UI_TRAIN_CHOICE
    #DIM LCOUNT
    #DIMS PRE
    #DIM CNUM 
    #DIM LNUM
    CNUM = 0
    LNUM = 0
    IF CON_UI(1,2) == 1
        FOR LCOUNT, 1, 100
            PRE = TRAINCHOICE%TOSTR(LCOUNT)%
            IF STR:PRE != ""
                IF LCOUNT+100*FLAG:SELECTEDTRAINGROUP == FLAG:SELECTEDTRAINCHOICE
                    CALL SET_COLOR("黄")
                    PRINTBUTTON @"%LEN_STR(STR:PRE,15)%" , LCOUNT+100*FLAG:SELECTEDTRAINGROUP
                    CALL PRINT_SPACE_1(2)
                    CNUM += 1
                    IF CNUM == 8
                        CNUM = 0
                        PRINTL
                        LNUM += 1
                    ENDIF
                    RESETCOLOR
                ELSE
                    PRINTBUTTON @"%LEN_STR(STR:PRE,15)%" , LCOUNT+100*FLAG:SELECTEDTRAINGROUP
                    CALL PRINT_SPACE_1(2)
                    CNUM += 1
                    IF CNUM == 8
                        CNUM = 0
                        PRINTL
                        LNUM += 1
                    ENDIF
                ENDIF
            ENDIF
        NEXT
        IF LNUM < 5
            CALL PRINT_LINE(5-LNUM)
        ENDIF
    ENDIF
    PRINTL

