@TRAIN_PUSH(ARGS)
    #DIM WNUM
    #DIM LNUM
    #DIM WNO = 0
    #DIM LCOUNT
    #DIMS PRE 
    #DIMS PRE_1
    SPLIT ARGS, "`", LOCALS
    WNO = 0
    LNUM = 15
    WNUM = RESULT
    FOR LCOUNT, LNUM - RESULT, 0, -1
        PRE = TRAINUI%TOSTR(LCOUNT + RESULT)%
        PRE_1 = TRAINUI%TOSTR(LCOUNT)%
        STR:PRE = %STR:PRE_1%
    NEXT
    FOR LCOUNT, RESULT, 0, -1
        PRE = TRAINUI%TOSTR(LCOUNT)%
        STR:PRE = %LOCALS:WNO%
        WNO += 1
    NEXT

