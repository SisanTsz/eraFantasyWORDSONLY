@UI_TRAIN
    #DIM LCOUNT
    IF CON_UI(1,2) == 1
        STR:UI = TRAIN
    ENDIF
    IF CON_UI(1,2,2) == 1
        CALL UI_TP(1,2,1,2)
        FLAG:SELECTEDCHARA2 = -1
    ENDIF
    IF STR:UI == "TRAIN"
        CALL UI_TRAIN_HEAD
        CALL UI_TRAIN_UI
        CALL UI_TRAIN_GROUP
        CALL UI_TRAIN_CHARALIST_1
        CALL UI_TRAIN_CHOICE
        CALL UI_TRAIN_CHARALIST_2
        CALL UI_TRAIN_BUTTON
    ENDIF

@UI_TRAIN_HEAD
    #DIM PNUM
    #DIM CNUM
    #DIM NNUM
    #DIM LCOUNT
    CUSTOMDRAWLINE -
    PRINTFORML %LOCATION(MASTER)% %MOUNTH()%月%DATE()%日 %HOUR()%时%MINUTE()%分
    PRINTL
    CNUM = 0
    NNUM = 0
    FOR LCOUNT, 0, CHARANUM
        IF LOCATION_CODE(LCOUNT) == LOCATION_CODE(MASTER)
            CNUM += 1
        ENDIF
    NEXT
    IF CNUM == 0
        PRINTL
    ELSEIF CNUM <=5
        FOR LCOUNT, 0, CHARANUM
            IF LOCATION_CODE(LCOUNT) == LOCATION_CODE(MASTER)
                IF NICKNAME:LCOUNT != ""
                    PRINTBUTTON @"[%NICKNAME:LCOUNT%]" , 2000 + LCOUNT
                ELSE
                    PRINTBUTTON @"[%NAME:LCOUNT%]" , 2000 + LCOUNT
                ENDIF
                CALL PRINT_SPACE_1(4)
            ENDIF
        NEXT
    ELSE
        FLAG:MAXCHARAPAGE = (CNUM-1)/5
        PRINTBUTTON "[-]" , 2301
        FOR LCOUNT, 0, CHARANUM
            IF LOCATION_CODE(LCOUNT) == LOCATION_CODE(MASTER) && LCOUNT != MASTER
                NNUM += 1
            ENDIF
            IF NNUM > 5*FLAG:CHARAPAGE && NNUM <= 5*(FLAG:CHARAPAGE + 1)
                IF NICKNAME:LCOUNT != ""
                    PRINTBUTTON @"[%NICKNAME:LCOUNT%]" , 2000 + LCOUNT
                ELSE
                    PRINTBUTTON @"[%NAME:LCOUNT%]" , 2000 + LCOUNT
                ENDIF
            ENDIF
        NEXT
        PRINTBUTTON "[-]" , 2302
    ENDIF
    CALL PRINT_LINE(1)
    CALL TRAIN_UI_PALAM
    ;IF FLAG:SELECTEDCHARA != -1
    ;    IF FLAG:SELECTEDTRAINCHOICE == -1
    ;        PRINTFORML %NAME:(FLAG:SELECTEDCHARA)% - 
    ;    ELSE
    ;        PRINTFORML %NAME:(FLAG:SELECTEDCHARA)% - %TRAINNAME:(FLAG:SELECTEDTRAINCHOICE)% - [   ]
    ;    ENDIF
    ;ELSE
    ;    PRINTL
    ;ENDIF
    CUSTOMDRAWLINE -

@TRAIN_UI_PALAM
    #DIM LCOUNT
    #DIM BAR_SIZE
    #DIM BAR_MAX
    #DIM BAR_COLOUR
    #DIM PALAM_MAX
    #DIM PCOUNT
    #DIM EX_LEVEL
    #DIM CNO
    PCOUNT = 0
    BAR_SIZE = 5
    BAR_COLOUR = 0x6666FF
    CNO = FLAG:CHARASTATUS
    PRINTL 【 快感 】
    FOR LCOUNT, 101, 111
        SELECTCASE PALAM:CNO:LCOUNT
            CASE IS >= 500000
                EX_LEVEL = 10
                PALAM_MAX = PALAM:CNO:LCOUNT + 1
            CASE IS >= 400000
                EX_LEVEL = 9
                PALAM_MAX = 500000
            CASE IS >= 300000
                EX_LEVEL = 8
                PALAM_MAX = 400000
            CASE IS >= 250000
                EX_LEVEL = 7
                PALAM_MAX = 300000
            CASE IS >= 200000
                EX_LEVEL = 6
                PALAM_MAX = 250000
            CASE IS >= 150000
                EX_LEVEL = 5
                PALAM_MAX = 200000
            CASE IS >= 100000
                EX_LEVEL = 4
                PALAM_MAX = 150000
            CASE IS >= 50000
                EX_LEVEL = 3
                PALAM_MAX = 100000
            CASE IS >= 20000
                EX_LEVEL = 2
                PALAM_MAX = 50000
            CASE IS >= 10000
                EX_LEVEL = 1
                PALAM_MAX = 20000
            CASE IS < 10000
                EX_LEVEL = 0
                PALAM_MAX = 10000
        ENDSELECT
        IF PALAMNAME:LCOUNT != ""
	        PRINTFORM %PALAMNAME:(LCOUNT)%LV:{EX_LEVEL}

	        CALL PRINT_BAR(PALAM:CNO:LCOUNT, PALAM_MAX, BAR_SIZE, BAR_COLOUR)
            PCOUNT += 1
            IF PCOUNT >= 3
                PRINTL
                PCOUNT = 0
            ENDIF
        ENDIF
    NEXT
    CALL PRINT_LINE(1)

