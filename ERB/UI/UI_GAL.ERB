@GAL_CHARA 
    #DIM PNUM
    #DIM CNUM
    #DIM NNUM
    #DIM LCOUNT
    CNUM = 0
    NNUM = 0
    FOR LCOUNT, 0, CHARANUM
        IF LOCATION_CODE(LCOUNT) == LOCATION_CODE(MASTER) && LCOUNT != MASTER
            CNUM += 1
        ENDIF
    NEXT
    IF CNUM == 0
        PRINTL
    ELSEIF CNUM <=5
        FOR LCOUNT, 0, CHARANUM
            IF LOCATION_CODE(LCOUNT) == LOCATION_CODE(MASTER) && LCOUNT != MASTER
                IF NICKNAME:LCOUNT != ""
                    PRINTBUTTON @"%NICKNAME:LCOUNT%" , 1300 + LCOUNT
                ELSE
                    PRINTBUTTON @"%NAME:LCOUNT%" , 1300 + LCOUNT
                ENDIF
                CALL PRINT_SPACE_1(4)
            ENDIF
        NEXT
    ELSE
        FLAG:MAXCHARAPAGE = (CNUM-1)/5
        PRINTBUTTON "[-]" , 1401
        FOR LCOUNT, 0, CHARANUM
            IF LOCATION_CODE(LCOUNT) == LOCATION_CODE(MASTER) && LCOUNT != MASTER
                NNUM += 1
            ENDIF
            IF NNUM > 5*FLAG:CHARAPAGE && NNUM <= 5*(FLAG:CHARAPAGE + 1)
                IF NICKNAME:LCOUNT != ""
                    PRINTBUTTON @"%NICKNAME:LCOUNT%" , 1300 + LCOUNT
                ELSE
                    PRINTBUTTON @"%NAME:LCOUNT%" , 1300 + LCOUNT
                ENDIF
            ENDIF
        NEXT
        PRINTBUTTON "[-]" , 1402
    ENDIF
    PRINTL
    CUSTOMDRAWLINE -